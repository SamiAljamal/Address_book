<h1>Contacts</h1>
<ul>
  @foreach (var contact in Model)
  {
    <li> Name: @contact.GetName()<br> Email:  @contact.GetEmail() <br> Number:  @contact.GetNumber()</li>
    <br>
  }
</ul>
<a href="/contacts/new">Create New Contact</a>
<form action="/contacts/deleted" method="post">
  <button type="submit"  class="btn btn-default center-block" >Clear Contacts</button>
</form>
